{
  "name": "@zh2s/react-utils",
  "version": "1.0.3",
  "description": "Сборник полезных утилит, хуков, HOCов для реакта",
  "main": "./build/index.js",
  "type": "module",
  "types": "./build/index.d.ts",
  "files": [
    "./build/"
  ],
  "scripts": {
    "build": "ts-node ./esbuild.ts && tsc",
    "fix:eslint": "eslint --fix ./src --max-warnings 0",
    "fix:prettier": "prettier --ignore-path .gitignore \"./src/**/*.{ts,tsx,scss,sass,css,md,mdx}\" --write",
    "fix:stylelint": "stylelint --fix \"./src/**/*.{scss,sass,css}\"",
    "fix:all": "npm run fix:eslint && npm run fix:prettier && npm run fix:stylelint",
    "pre-commit": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run fix:eslint"
    ],
    "src/**/*.{ts,tsx,scss,sass,css,md,mdx}": [
      "npm run fix:prettier"
    ],
    "src/**/*.{scss,sass,css}": [
      "npm run fix:stylelint"
    ]
  },
  "keywords": [
    "React",
    "hooks",
    "HOC"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ZholobovSS/RP_DAY_7_lib.git"
  },
  "homepage": "https://github.com/ZholobovSS/RP_DAY_7_lib",
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "esbuild": "0.25.11",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "eslint-plugin-react-refresh": "0.4.24",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "prettier": "3.6.2",
    "stylelint": "16.25.0",
    "stylelint-config-recess-order": "7.4.0",
    "stylelint-config-standard": "39.0.1",
    "stylelint-config-standard-scss": "16.0.0",
    "stylelint-order": "7.0.0",
    "stylelint-prettier": "5.0.3",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2"
  },
  "peerDependencies": {
    "react": ">=18.3.1",
    "react-dom": ">=18.3.1",
    "react-router": ">=7.9.4"
  },
  "author": "Zholobov Semyon Sergeevich <mail@mail.ru>",
  "license": "ISC"
}
